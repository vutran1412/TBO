<!DOCTYPE html>
<html>
  <head>
    <title>TBO</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.splashImage = "<%= image_path('splash.png') %>"
      window.btnPlayMedHovered = "<%= image_path('btn_play_med_hovered.png') %>"
      window.icnImagePlay = "<%= image_path('icn_player_play') %>"
      window.logoImage = "<%= image_path('tbo-06.png') %>"
      window.userDefault = "<%= image_path('user-default.png') %>"
      window.userHover = "<%= image_path('user-hover.png') %>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
